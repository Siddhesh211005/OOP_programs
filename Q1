class Animal{
    void makesound(){
        System.out.println("Animal make sound");
    }


}
class dog extends Animal{

    @Override
    void makesound() {
        System.out.println("Dog barks");
    }
}


public class main {
    public static void main(String[] args){

        dog d = new dog();
        d.makesound();
    }
}
---------------OUTPUT-----------------
Dog barks
